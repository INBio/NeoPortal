<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.inbio.neoportal.core.entity.Images" table="images">
    <id name="imageId" type="big_decimal" column="image_id">
        <generator class="sequence" />
    </id>
    <property name="m3sImageId" type="big_decimal" column="m3s_image_id" />
    <property name="author" type="string" column="author" />
    <property name="rights" type="string" column="rights" />
    <many-to-one name="taxon" class="org.inbio.neoportal.core.entity.Taxon" fetch="select" column="taxon_id" />
  </class>
</hibernate-mapping>
